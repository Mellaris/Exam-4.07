<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="250"
        x:Class="AvaloniaApplication1.ChangeRang"
		x:CompileBindings="False"
        Title="ChangeRang">

	<StackPanel>
		<TextBlock Margin="0,30,0,0" HorizontalAlignment="Center">Изменение ранга</TextBlock>
		<TextBox Name="rang" Margin="0,30,0,0" Width="50"></TextBox>
		<Button Click="EditRang" Margin="0,30,0,0" HorizontalAlignment="Center">
			<TextBlock>Изменить ранг</TextBlock>
		</Button>
	</StackPanel>
	
</Window>
